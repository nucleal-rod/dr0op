{
  "name": "image2d",
  "version": "1.4.8",
  "description": "🍇 使用ECMAScript绘制二维图片。Drawing Two-Dimensional Pictures Using ECMAScript.",
  "main": "build/image2D.min.js",
  "scripts": {
    "start": "npm install && node ./task/explain.js",
    "build": "cuf -d ./build && node ./task/module.js && node ./task/babel.js && node ./task/uglify.js",
    "support": "npx browserslist",
    "postinstall": "node ./script/postinstall.js || echo \"ignore\""
  },
  "keywords": [
    "image",
    "algorithm",
    "svg",
    "canvas"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yelloxing/image2D.git"
  },
  "author": "心叶",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yelloxing/image2D/issues"
  },
  "homepage": "https://yelloxing.github.io/image2D",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^8.0.0",
    "cuf": "^1.0.3",
    "rollup": "^1.17.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "uglify-js": "^3.5.6"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "ie > 8",
    "chrome >= 61",
    "firefox >= 65"
  ],
  "dependencies": {
    "@yelloxing/core.js": "^0.3.1"
  }
}
